{
  "version": 2,
  "builds": [
    {
      "src": "api/dua.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/dua",
      "dest": "/api/dua.js"
    },
    {
      "src": "/bingo",
      "dest": "/bingo.html"
    },
    {
      "src": "/dua",
      "dest": "/dua.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
